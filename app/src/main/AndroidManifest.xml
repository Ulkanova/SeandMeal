<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ulkanova">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SeandMeal">
        <receiver
            android:name=".PedidoReceiver"
            android:enabled="true"
            android:exported="true"></receiver>
        <service
            android:name=".MyIntentServices"
            android:exported="false" />
        <activity
            android:name=".PedidoActivity"
            android:parentActivityName=".ListaPlatos" />
        <activity
            android:name=".ListaPlatos"
            android:label="@string/title_activity_lista_platos"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".NuevoPlato"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".MainActivity"
            android:parentActivityName=".HomeActivity" />
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
</manifest>